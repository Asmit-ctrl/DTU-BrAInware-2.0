{
  "name": "parent-student-portal",
  "version": "1.0.0",
  "description": "Parent Student Login Portal with Quiz Platform",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
