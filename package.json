    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.8.8",
    "node-fetch": "^2.6.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.0",
    "nodemon": "^3.0.0"
  }
}
